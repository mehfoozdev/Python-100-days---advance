     ]
    }
   ],
   "source": [
    "num = int(input('Enter a number : '))\n",
    "s = 0\n",
    "\n",
    "while num > 0:\n",
    "    d = num  % 10\n",
    "    s += d\n",
    "    num = num // 10\n",
    "print(\"Sum of digits is : \", s)\n",
    "    "
